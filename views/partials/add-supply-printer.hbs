<div class="manage">
    <button class="manage-btn btn">add supplies</button>
    button
    {{#each @root.suppliesForPrinter}}
        <form action="/supply-printer" method="post" class="supply">
            <label for="" class="supply__label">{{supply}}</label>
            <input type="hidden" name="printerId" class="supply__input" value="{{@root.printer._id}}">
            <input type="hidden" name="code" value="{{code}}">
            {{#unless av}}
                <button class="supply__button">add</button>
            {{else}}
                <button class="supply__button" disabled>add</button>
            {{/unless}}
        </form>

    {{/each}}
</div>